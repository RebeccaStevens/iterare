{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2016", "esnext.asynciterable", "dom"],
    "outDir": "lib",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedLocals": true
  },
  "include": ["src/**/*.ts"]
}
